#include<iostream>
using namespace std;
int main()
{
	int a[10];
	int s=0, i=10;
	cout<<"This Program will calculate the sum of the squires of the first 10 integers"<<endl;
	cout<<"Please insert the 10 Numbers one by one"<<endl;
	for(i=0; i<10; i++)
	{
		cin>>a[i];
	}
	for(i=0; i<10; i++)
	{
		s=s+a[i]*a[i];
		
	}
	cout<<"The sum of the squires of the 10 Number is "<<s<<endl;
	return 0;
	
}
