#include<iostream>
using namespace std;
int main()
{
	int i, z;
	int a[100];
	for(i=0; i<100; i++)
		{
			a[i]=i;
		}
	cout<<"Please insert the number for Guessing"<<endl;
	cin>>z;
	int f=0;
	for(i=0; i<100; i++)
		{
			if(z==a[i])
			{
				f=1;
				break;	
			}
		}
	if(f==1)
		{
			cout<<"Congruglation Number found, the number is "<<i<<endl;
		}
	else
		{
			cout<<"The number was not found";
		}
	return 0;
}
