#include<iostream>
#include<stdlib.h>
using namespace std;
int main()
{
	int i, z, f=0;
	int a[100];
	for(i=0; i<100; i++)
	{
		a[i]=rand();
	}
	cout<<"Please insert the number for Guessing"<<endl;
	cin>>z;
	for(i=0; i<100; i++)
	{
		if(z==a[i])
		{
			f=1;
			break;
		}
		
	}
	if(f==1)
	{
		cout<<"Congruglation You found the Number ="<<i;
	}
	else
	cout<<"The Number was not found";

	return 0;
	
}
