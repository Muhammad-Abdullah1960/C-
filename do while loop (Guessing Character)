// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() 
{
    int num=0;
    char c;

    do
    {
        cout<<"Please input character b/w a-z for guess"<<endl;
        cin>>c;   
        if(c=='z')
        {
            cout<<"Conguralation! your Guess is correct";
            num=6;
        }
        else
        {
            num=num+1;

        }
    
    }
    while(num<=5);
cout<<"...."<<endl;
cout<<"Thanks for using This Program";
    return 0;
}
